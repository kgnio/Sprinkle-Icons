{
  "name": "sprinkle-icons",
  "displayName": "Sprinkle Icons",
  "description": "An icons theme pack for Visual Studio Code.",
  "version": "1.0.0",
  "publisher": "kagan-dev",
  "scripts": {
    "gen:icons": "node scripts/generate-icons.cjs",
    "gen:placeholders": "node scripts/generate-placeholders.cjs",
    "gen:theme": "node scripts/manifest-to-theme.cjs",
    "check-icons": "node scripts/check-icons.cjs",
    "normalize-theme": "node scripts/normalize-theme.cjs",
    "scan:manifest": "node scripts/scan-manifest.cjs",
    "populate-maps": "node scripts/populate-maps.js",
    "update:themes": "node scripts/update-contributions.js",
    "suggest:icons": "node scripts/iconSuggester.js",
    "start": "node scripts/index.cjs && npm run update:themes"
  },
  "engines": {
    "vscode": "^1.80.0"
  },
  "categories": [
    "Themes"
  ],
  "contributes": {
    "iconThemes": [
      {
        "id": "arcade",
        "label": "Arcade",
        "path": "./themes/arcade.json"
      },
      {
        "id": "new",
        "label": "New",
        "path": "./themes/new.json"
      },
      {
        "id": "sakura",
        "label": "Sakura",
        "path": "./themes/sakura.json"
      },
      {
        "id": "suggestion",
        "label": "Suggestion",
        "path": "./themes/suggestion.json"
      }
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.28.0",
    "@babel/preset-env": "^7.28.0",
    "@babel/preset-react": "^7.27.1",
    "@babel/register": "^7.27.1",
    "cross-env": "^7.0.3",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3"
  },
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react"
    ]
  },
  "dependencies": {
    "fs": "^0.0.1-security",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-icons": "^5.5.0"
  }
}